<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Events JSON</title>
</head>
<body>
<script>
  // Simulate an API endpoint that returns JSON
  const events = [
    {
      title: "Team Meeting",
      start: "2025-11-05T10:00:00",
      end: "2025-11-05T11:00:00"
    },
    {
      title: "Client Presentation",
      start: "2025-11-07T14:00:00",
      end: "2025-11-07T15:30:00"
    },
    {
      title: "Code Review Session",
      start: "2025-11-10T09:00:00",
      end: "2025-11-10T10:30:00"
    },
    {
      title: "Project Deadline",
      start: "2025-11-15",
      end: "2025-11-15"
    }
  ];

  // Return JSON response
  document.write(JSON.stringify(events, null, 2));
</script>
</body>
</html>
